                        ul.treeview-menu
                            li
                                a(href='/api/v1/extensions?access_token=' + token) Extension
                            li
                                a(href='/api/v1/organize?access_token=' + token) Groups
                            li
                                a(href='/api/v1/trunk?access_token=' + token) Trunks
                    li.treeview
                        a(href='#')
                            i.fa.fa-btc
                            span Tariffs
